hasChanges() {
	test -n "$(git status -s .)"
}

getDockerArch() {
        local arch
        arch=$(uname -m)
        if [[ "${arch}" = "x86_64" ]]; then
                echo "amd64"
        elif [[ "${arch}" = "armv7l" ]]; then
                echo "arm"
        else
                echo "UNKNOWN-ARCH-${arch}"
        fi
}

getVersion() {
	cat .release
}
